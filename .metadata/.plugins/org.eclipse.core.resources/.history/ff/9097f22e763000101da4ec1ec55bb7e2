$(function(){
	
	$('#place-saved').click(function(){
		
		$('#content-themes').hide();
		$('#content-reviews').hide();
		
		$('#content-saved').show();
		

	})
	
	$('#tema-saved').click(function(){
		
		$('#content-reviews').hide();
		$('#content-saved').hide();
		
		$('#content-themes').show();
	})

	$('#review-saved').click(function(){
		
		
		$('#content-themes').show();
	})
})